<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="100.0" prefWidth="200.0" styleClass="layout" stylesheets="@../styles/addBoard.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.controllers.popups.EditBoardController">
    <TextField fx:id="board_name" layoutX="50.0" layoutY="29.0" prefWidth="100.0" />
    <Button fx:id="done_button" onAction="#save" layoutX="29.0" layoutY="59.0" mnemonicParsing="false" text="Save" />
    <Button fx:id="back_button" onAction="#back" layoutX="121.0" layoutY="59.0" mnemonicParsing="false" text="Cancel" />
    <Text layoutX="30.0" layoutY="27.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Enter updated board name:" />
</AnchorPane>
